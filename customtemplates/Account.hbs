<!-- BEGIN CONTENT BODY -->
<div class="page-content" style="padding-top:0">
    <div class="row">
      <div class="col-md-12">
        <div class="portlet light bordered">
            <div class="portlet-title">
                <div class="caption">
                    <i class="{{icon}} font-dark"></i>
                    <span class="caption-subject font-dark sbold uppercase">  {{Name}}</span>
                    <input type="hidden" id="cType" value="{{cType}}">
                </div>
                <span class="font-dark sbold uppercase componentDetails">{{Type}} Details</span>
            </div>
            <div class="portlet-body form">
                <form class="form-horizontal compForm" role="form">
                    <div class="form-body">
                        <div class="form-group">
                            <label class="col-md-3 control-label">Name</label>
                            <div class="col-md-9">
                                <input type="text" class="form-control" placeholder="{{Type}} Adi" value="{{Name}}" name="Name">
                            </div>
                        </div>
                        <div class="form-group">
                          <label class="col-md-3 control-label">Is Extension?</label>
                            <div class="col-md-9">
                              <label class="mt-checkbox">
                                  {{#if IsExtension}}
                                    <input type="checkbox" class="form-control isExtCheckbox" checked>
                                  {{else}}
                                    <input type="checkbox" class="form-control isExtCheckbox">
                                  {{/if}}
                                  <span></span>
                              </label>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Extension</label>
                            <div class="col-md-9">
                              {{#if IsExtension}}
                                <input type="text" class="form-control isExtensionVar" placeholder="Extension" value="{{Extension}}" name="Extension">
                                {{else}}
                                <input type="text" class="form-control isExtensionVar" placeholder="Extension" value="{{Extension}}" name="Extension" disabled>
                              {{/if}}
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">RegistrarServer</label>
                            <div class="col-md-9">
                              {{#if IsExtension}}
                                <input type="text" class="form-control isExtensionVar" placeholder="RegistrarServer" value="{{RegistrarServer}}" name="RegistrarServer">
                                {{else}}
                                <input type="text" class="form-control isExtensionVar" placeholder="RegistrarServer" value="{{RegistrarServer}}" name="RegistrarServer" disabled>
                              {{/if}}
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Localnumber</label>
                            <div class="col-md-9">
                              {{#if IsExtension}}
                                <input type="text" class="form-control isExtensionVar2" placeholder="Localnumber" value="{{Localnumber}}" name="Localnumber" disabled>
                                {{else}}
                                <input type="text" class="form-control isExtensionVar2" placeholder="Localnumber" value="{{Localnumber}}" name="Localnumber">
                              {{/if}}
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">User Name</label>
                            <div class="col-md-9">
                              <input type="text" class="form-control" placeholder="UserName" value="{{UserName}}" name="UserName">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Password</label>
                            <div class="col-md-9">
                              <input type="password" class="form-control" placeholder="Password" value="{{Password}}" name="Password">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Realm</label>
                            <div class="col-md-9">
                              <input type="text" class="form-control" placeholder="Realm" value="{{Realm}}" name="Realm">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Schema</label>
                            <div class="col-md-9">
                              <input type="text" class="form-control" placeholder="Schema" value="{{Schema}}" name="Schema">
                            </div>
                        </div>
                        <div class="form-group">
                          <label class="col-md-3 control-label">Behind Nat</label>
                            <div class="col-md-9">
                              <label class="mt-checkbox">
                                  {{#if BehindNat}}
                                    <input type="checkbox" class="form-control behindNatCheckbox" checked>
                                  {{else}}
                                    <input type="checkbox" class="form-control behindNatCheckbox">
                                  {{/if}}
                                  <span></span>
                              </label>
                            </div>
                        </div>
                        <div class="form-group">
                          <label class="col-md-3 control-label">Has Proxy?</label>
                            <div class="col-md-9">
                              <label class="mt-checkbox">
                                  {{#if HasProxy}}
                                    <input type="checkbox" class="form-control hasProxyCheckbox" checked>
                                  {{else}}
                                    <input type="checkbox" class="form-control hasProxyCheckbox">
                                  {{/if}}
                                  <span></span>
                              </label>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Proxy Server</label>
                            <div class="col-md-9">
                              {{#if HasProxy}}
                                <input type="text" class="form-control hasProxyVar" placeholder="ProxyServer" value="{{ProxyServer}}" name="ProxyServer">
                                {{else}}
                                <input type="text" class="form-control hasProxyVar" placeholder="ProxyServer" value="{{ProxyServer}}" name="ProxyServer" disabled>
                              {{/if}}
                            </div>
                        </div>
                        <div class="form-group">
                          <label class="col-md-3 control-label">Has Stun Server?</label>
                            <div class="col-md-9">
                              <label class="mt-checkbox">
                                  {{#if HasStunServer}}
                                    <input type="checkbox" class="form-control hasStunServerCheckbox" checked>
                                  {{else}}
                                    <input type="checkbox" class="form-control hasStunServerCheckbox">
                                  {{/if}}
                                  <span></span>
                              </label>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Stun Server</label>
                            <div class="col-md-9">
                              {{#if HasStunServer}}
                                <input type="text" class="form-control hasStunServerVar" placeholder="StunServer" value="{{StunServer}}" name="StunServer">
                                {{else}}
                                <input type="text" class="form-control hasStunServerVar" placeholder="StunServer" value="{{StunServer}}" name="StunServer" disabled>
                              {{/if}}
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Capacity</label>
                            <div class="col-md-9">
                                <input type="number" class="form-control" placeholder="Capacity" min="0" max="10000" value="{{Capacity}}" name="Capacity">
                            </div>
                        </div>
                    </div>
                    <div class="form-actions">
                        <div class="row">
                            <div class="col-md-offset-7 col-md-5 text-right">
                                <a class="removeComp1 btn red-soft" data-type={{Type}} data-id="{{Id}}" style="display:inline-block">Remove</a>
                                <a class="resetComp1 btn green-soft" data-type={{Type}} data-id="{{Id}}" style="display:inline-block">Cancel</a>
                              <button type="submit" class="btn  blue-madison" data-type={{Type}} data-id="{{Id}}">Save</button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
      </div>
      </div>
    </div>
</div>
<!-- END CONTENT BODY -->
