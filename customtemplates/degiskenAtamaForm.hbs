<div class="portlet portlet-sortable light bordered formPortlet {{nodeId}}" data-form-type="{{inFormData.Type}}">
  <div class="portlet-title">
      <div class="caption {{fontClass}}">
          <span class="caption-subject bold uppercase"> Degisken Atama Formu</span>
      </div>
      <div class="tools">
          <a href="" class="collapse" data-original-title="" title=""> </a>
          <a href="" class="remove" data-original-title="" title="" data-target-node="{{nodeId}}"> </a>
      </div>
  </div>
<div class="portlet-body">
  <div class="table-toolbar">
       <div class="row">
           <div class="col-md-6">
               <div class="btn-group">
                   <button id="{{inFormData.btnID}}" class="btn green"> Yeni Degisken
                       <i class="fa fa-plus"></i>
                   </button>
               </div>
           </div>
       </div>
   </div>
   <table class="table table-striped table-hover table-bordered" id="{{inFormData.tableID}}">
       <thead>
           <tr>
               <th> Variable Definition </th>
               <th> Varible Value </th>
               <th> Edit </th>
               <th> Delete </th>
           </tr>
       </thead>
       <tbody>
         {{#each inFormData.Assignments}}
           <tr>
               <td> {{VariableDefinition}} </td>
               <td class="center">{{VaribleValue}}</td>
               <td>
                   <a class="edit" href="javascript:;"> Edit </a>
               </td>
               <td>
                   <a class="delete" href="javascript:;"> Delete </a>
               </td>
           </tr>
         {{/each}}
       </tbody>
   </table>
</div>
</div>
