<div class="portlet portlet-sortable light bordered formPortlet {{nodeId}}" data-form-type="{{inFormData.Type}}">
    <div class="portlet-title">
        <div class="caption {{fontClass}}">
            <span class="caption-subject bold uppercase"> CTI Form - ( {{inFormData.CtiEntities.length}} )</span>
        </div>
        <div class="tools">
            <a href="" class="collapse" data-original-title="" title=""> </a>
            <a href="" class="remove" data-original-title="" title="" data-target-node="{{nodeId}}"> </a>
        </div>
    </div>
    <div class="portlet-body">
        <form class="form-horizontal ctiForm" role="form">
            <div class="form-body">
                <div class="form-group">
                    <div class="col-md-12">
                        <div class="table-toolbar">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="btn-group">
                                        <button id="{{inFormData.btnID}}" class="btn green newCtiData">
                                            New CTI Data
                                            <i class="fa fa-plus"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <table class="table table-striped table-hover table-bordered" id="{{inFormData.tableID}}">
                            <thead>
                                <tr>
                                    <th> CTI Data </th>
                                    <th> CTI Value </th>
                                    <th> CTI Function </th>
                                    <th> CTI Sip Operation </th>
                                    <th> Edit </th>
                                    <th> Delete </th>
                                </tr>
                            </thead>
                            <tbody>
                                {{#each inFormData.CtiEntities}}
                                <tr>
                                    <td>
                                        <input type="text" class="form-control " value="{{CtiData}}" disabled>
                                        <input type="hidden" class="form-control " value="{{CtiData}}">
                                    </td>
                                    <td>
                                        <input type="text" class="form-control " value="{{CtiValue}}" disabled>
                                        <input type="hidden" class="form-control " value="{{CtiValue}}">
                                    </td>
                                    <td>
                                        <select class="js-table-select ctiFunction">
                                            <option value="0">Get</option>
                                            <option value="1">Set</option>
                                        </select>
                                        <input type="hidden" class="form-control" value="{{CtiFunction}}">
                                    </td>
                                    <td>
                                        <select class="js-table-select ctiSipOperation">
                                            <option value="0">None</option>
                                            <option value="1">Add Info</option>
                                            <option value="2">Send Info</option>
                                            <option value="3">Send Bye</option>
                                            <option value="4">Send Transfer</option>
                                        </select>
                                        <input type="hidden" class="form-control" value="{{CtiSipOperation}}">
                                    </td>
                                    <td>
                                        <a class="editRow" href="#"> Edit </a>
                                    </td>
                                    <td>
                                        <a class="deleteRow" href="#"> Delete </a>
                                    </td>
                                </tr>
                                {{/each}}
                            </tbody>
                        </table>
                        <input type="hidden" name="newRowAdded" value="" />
                        <input type="hidden" name="rowEdited" value="" />
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>
